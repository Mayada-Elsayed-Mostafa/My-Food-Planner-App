<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="MainButton" parent="Widget.AppCompat.Button">
        <item name="android:textColor">@color/white</item>
        <item name="android:textSize">@dimen/_16ssp</item>
    </style>


    <style name="TitleTextStyle">
        <item name="android:textSize">@dimen/_18ssp</item>
        <item name="android:textColor">@color/black</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="ItemTextStyle">
        <item name="android:textSize">@dimen/_14ssp</item>
        <item name="android:textColor">@color/black</item>
    </style>


    <style name="FABShape">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>
</resources>